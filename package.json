{
  "name": "frontendBoilerplate",
  "version": "0.0.0",
  "description": "basic blank project setup. Hopefully saves you time configuring",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "bundle": "webpack && npm run assetBundle && cp ./index.html ./dist && cp -r ./src/images ./dist/",
    "clean": "rm -rf ./dist",
    "assetBundle": "cp -r ./src/testData ./dist/",
    "lint": "tslint --force 'src/**/*.ts?' > ./tslint.err",
    "serve": "npm run clean && npm run lint && npm run bundle && cat ./tslint.err && webpack-dev-server --content-base ./dist/bundle.js --no-inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkdigitaldream/frontendBoilerplate.git"
  },
